<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet id="1.0-init-dict-data" author="settler">

        <insert tableName="mod_settlement_type">
            <column name="code">NOR</column>
            <column name="description">Normal</column>
        </insert>
        <insert tableName="mod_settlement_type">
            <column name="code">OTH</column>
            <column name="description">Other</column>
        </insert>

        <insert tableName="mod_transaction_type">
            <column name="code">NOR</column>
            <column name="description">Normal</column>
        </insert>
        <insert tableName="mod_transaction_type">
            <column name="code">OTH</column>
            <column name="description">Other</column>
        </insert>

        <rollback>
            <delete tableName="mod_settlement_type"/>
            <delete tableName="mod_transaction_type"/>
        </rollback>

    </changeSet>


</databaseChangeLog>